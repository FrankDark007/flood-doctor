{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "mclean.flood.doctor" }],
      "destination": "/city/mclean"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "vienna.flood.doctor" }],
      "destination": "/city/vienna"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "arlington.flood.doctor" }],
      "destination": "/city/arlington"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "fairfax.flood.doctor" }],
      "destination": "/city/fairfax"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "tysons.flood.doctor" }],
      "destination": "/city/tysons"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "alexandria.flood.doctor" }],
      "destination": "/city/alexandria"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "ashburn.flood.doctor" }],
      "destination": "/city/ashburn"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "dc.flood.doctor" }],
      "destination": "/city/washington-dc"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "herndon.flood.doctor" }],
      "destination": "/city/herndon"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "reston.flood.doctor" }],
      "destination": "/city/reston"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "springfield.flood.doctor" }],
      "destination": "/city/springfield"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "fallschurch.flood.doctor" }],
      "destination": "/city/falls-church"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini-api-key"
  }
}
