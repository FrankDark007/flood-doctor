{
  "name": "flood-doctor-google-redesign-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run generate:sitemaps",
    "build:city": "VITE_BUILD_MODE=city vite build --outDir dist-city",
    "build:cities": "npm run build:city && npx tsx scripts/build-cities.ts",
    "generate:sitemaps": "npx tsx scripts/generate-sitemaps.ts",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:install": "playwright install",
    "e2e:visual": "playwright test --grep 'Google Home Clone Visual' --project=chromium",
    "e2e:visual:update": "playwright test --grep 'Google Home Clone Visual' --project=chromium --update-snapshots",
    "e2e:visual:report": "playwright show-report",
    "e2e:parity": "playwright test e2e/google-home-clone-parity.spec.ts --project=chromium",
    "e2e:parity:update": "GBP_PARITY_MODE=reference playwright test e2e/google-home-clone-parity.spec.ts --project=chromium --update-snapshots",
    "e2e:parity:report": "playwright show-report",
    "e2e:parity:debug": "playwright test e2e/google-home-clone-parity.spec.ts --grep 'side-by-side' --project=chromium",
    "e2e:behavior": "playwright test e2e/google-home-clone-behavior.spec.ts --project=chromium",
    "parity:no-external": "npx tsx scripts/parity/verify-no-external-requests.ts",
    "parity:scan-dist": "npx tsx scripts/parity/scan-dist-external-urls.ts",
    "parity:layout": "npx tsx scripts/parity/verify-fd-layout-invariants.ts",
    "parity:diff": "npx tsx scripts/parity/fd-home-v3-diff.ts",
    "parity:all": "npm run parity:no-external && npm run parity:scan-dist"
  },
  "dependencies": {
    "@fontsource-variable/plus-jakarta-sans": "^5.2.8",
    "@google/genai": "^1.34.0",
    "autoprefixer": "^10.4.23",
    "framer-motion": "^12.24.0",
    "lucide-react": "^0.560.0",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.10.1",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^4.0.16",
    "basic-ftp": "^5.1.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.4.2",
    "sirv": "^3.0.2",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.51.0",
    "vite": "^6.2.0",
    "vitest": "^4.0.16"
  }
}
